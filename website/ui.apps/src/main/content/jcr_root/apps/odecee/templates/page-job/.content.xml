<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:description="An Odecee job ad"
	jcr:primaryType="cq:Template" jcr:title="Odecee Job page"
	allowedPaths="[/content/odecee/.+/locations/.+/jobs]" ranking="{Long}100">
	<jcr:content jcr:primaryType="cq:PageContent"
		sling:resourceType="odecee/components/structure/jobpage">
		<nav_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
        </nav_section>
		<joboverview jcr:primaryType="nt:unstructured"
			sling:resourceType="odecee/components/content/text" />
		<jobdescription jcr:primaryType="nt:unstructured"
			sling:resourceType="odecee/components/content/text" />
		<qualifications jcr:primaryType="nt:unstructured"
			sling:resourceType="odecee/components/content/text" />
		<footer_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
        </footer_section>
	</jcr:content>
</jcr:root>
