<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Template for initial or main page of the site."
    jcr:primaryType="cq:Template"
    jcr:title="odecee Home Page"
    allowedPaths="[/content/odecee]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="odecee/components/structure/page">
        <nav_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/iparsys">
            <navbar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="odecee/components/structure/navbar"/>
        </nav_section>
        <main_section
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys"/>
        <footer_section
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/iparsys">
            <footer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="odecee/components/structure/footer"/>
        </footer_section>
    </jcr:content>
</jcr:root>
