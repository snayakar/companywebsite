<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="odecee Design"
        sling:resourceType="wcm/core/components/designer">
        <page jcr:primaryType="nt:unstructured">
            <main_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys"
                components="[group:Odecee Components]">
                <colctrl
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="odecee/components/content/colctrl"
                    layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)&#xa;3;cq-colctrl-lt1&#x9;3 Columns (33%, 33%, 33%)&#xa;4;cq-colctrl-lt2&#x9;4 Columns (25%, 25%, 25%, 25%)"/>
                <cq:authoring
                    jcr:primaryType="nt:unstructured">
                    <assetToComponentMapping jcr:primaryType="nt:unstructured">
                        <image
                            jcr:primaryType="nt:unstructured"
                            assetGroup="media"
                            assetMimetype="image/*"
                            droptarget="image"
                            resourceType="odecee/components/content/image"
                            type="Images"/>
                    </assetToComponentMapping>
                </cq:authoring>
            </main_section>
            <footer_section
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/parsys"
                    components="[group:Odecee Components]">
                <colctrl
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="odecee/components/content/colctrl"
                        layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)&#xa;3;cq-colctrl-lt1&#x9;3 Columns (33%, 33%, 33%)&#xa;4;cq-colctrl-lt2&#x9;4 Columns (25%, 25%, 25%, 25%)"/>
                <cq:authoring
                        jcr:primaryType="nt:unstructured">
                    <assetToComponentMapping jcr:primaryType="nt:unstructured">
                        <image
                                jcr:primaryType="nt:unstructured"
                                assetGroup="media"
                                assetMimetype="image/*"
                                droptarget="image"
                                resourceType="odecee/components/content/image"
                                type="Images"/>
                    </assetToComponentMapping>
                </cq:authoring>
            </footer_section>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="odecee/components/structure/logo"
                absParent="2">
                <image/>
            </logo>
        </page>
    </jcr:content>
</jcr:root>
